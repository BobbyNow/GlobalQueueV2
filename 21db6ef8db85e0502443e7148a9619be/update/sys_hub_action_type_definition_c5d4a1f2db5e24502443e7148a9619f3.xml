        <sys_updated_on>2021-03-29 19:30:33</sys_updated_on>
        <value>(function execute(inputs, outputs) {
  var task = new GlideRecord('task');
  if (task.get(inputs.task)) {
  	var gr = new GlideRecord('task_sla');
	gr.addQuery('task', inputs.task);
	gr.addQuery('end_time', '');
	gr.orderBy('time_left');
	gr.setLimit(1);
	gr.query();
	if (gr.next()) {
		task.x_snc_gq2_next_sla_due = gr.time_left;
	} else {
		task.x_snc_gq2_next_sla_due = '';
    }
	task.update();
   }
})(inputs, outputs);</value>
